
@{
    ViewData["Title"] = "ShowTravel";
}

<h1>ShowTravel</h1>

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <td>Name</td>
            <td>Address</td>
            <td>Tel</td>
        </tr>
    </thead>
    <tbody>

    </tbody>
</table>


@section Scripts{ 
    <script src="~/js/travel.js"></script>
    <script>
        let str = "";
        for (let i = 0; i < DATAS.length; i++) {
            str += `<tr><td>${DATAS[i].Name}</td>`;
            str += `<td>${DATAS[i].Add}</td>`;
            str += `<td>${DATAS[i].Tel}</td></tr>`;
        }
        $("tbody").html(str);
    </script>
}